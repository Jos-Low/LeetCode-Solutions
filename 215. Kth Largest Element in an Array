import heapq
nums = [3,2,3,1,2,4,5,5,6]
k = 4

# Heap Solution
heap = []
for n in nums:
    heapq.heappush(heap, n)
    if len(heap) > k:
        heapq.heappop(heap)
print(heap[0])


# Failed TLE approach
